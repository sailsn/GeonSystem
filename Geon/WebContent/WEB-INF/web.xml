<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Geon</display-name>
  <welcome-file-list>
   
    <welcome-file>login.jsp</welcome-file>

  </welcome-file-list>
  <servlet>
  <servlet-name>ListofComplaints</servlet-name>
  <servlet-class>com.geon.controller.UserController</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>ListofComplaints</servlet-name>
  <url-pattern>/UserController</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>ListOfUsers</servlet-name>
  <servlet-class>com.geon.controller.AdminController</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>ListOfUsers</servlet-name>
  <url-pattern>/AdminController</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>Login</servlet-name>
  <servlet-class>com.geon.controller.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>Login</servlet-name>
  <url-pattern>/LoginController</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>PieChartServlet</servlet-name>
  <servlet-class>com.geon.controller.PieChartController</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>PieChartServlet</servlet-name>
  <url-pattern>/PieChartController</url-pattern>
  </servlet-mapping>
  
  
  
  
</web-app>